SHOWLOG_OK_STR="Show log OK!"

source ${RESCAPP_LIB_FILE}

nohup ${FILEMANAGER_COMMAND} file://${RESCAPP_LOG_DIRECTORY} &disown


rtux_Message_Success ${SHOWLOG_OK_STR}

